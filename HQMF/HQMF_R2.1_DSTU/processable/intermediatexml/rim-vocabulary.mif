				<conceptProperty name="internalId" value="11616"/>
				<conceptProperty name="isDocumentCharacteristic" value="true"/>
				<conceptProperty name="Name:Act:outboundRelationship:ActRelationship" value="predecessor"/>
				<conceptProperty name="Sort:Act:outboundRelationship:ActRelationship" value="BIA______"/>
				<conceptProperty name="Name:Act:inboundRelationship:ActRelationship" value="successor"/>
				<conceptProperty name="Sort:Act:inboundRelationship:ActRelationship" value="BIA______"/>
				<printName language="en" preferredForLanguage="true" text="succeeds"/>
				<code code="SUCC" status="active"/>
			</concept>
			<concept>
				<annotations>
					<documentation>
						<definition>
							<text>
								<p>A condition thread relationship specifically links condition nodes together to form a condition thread. The source is the new condition node and the target links to the most recent node of the existing condition thread.</p>
							</text>
						</definition>
					</documentation>
				</annotations>
				<conceptRelationship relationshipName="Specializes">
					<targetConcept code="SEQL"/>
				</conceptRelationship>
