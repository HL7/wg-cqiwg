			</supportedConceptProperty>
			<concept isSelectable="false">
				<annotations>
					<documentation>
						<definition>
							<text>
								<p>The association adds to the existing context associated with the Act.  Both this association and any associations propagated from ancestor Acts are interpreted as being related to this Act.</p>
							</text>
						</definition>
					</documentation>
				</annotations>
				<conceptRelationship relationshipName="Generalizes" isDerived="true">
					<targetConcept code="AN"/>
				</conceptRelationship>
				<conceptRelationship relationshipName="Generalizes" isDerived="true">
					<targetConcept code="AP"/>
				</conceptRelationship>
