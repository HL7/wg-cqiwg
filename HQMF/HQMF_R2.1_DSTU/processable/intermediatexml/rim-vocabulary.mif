							</text>
						</definition>
					</documentation>
				</annotations>
				<conceptRelationship relationshipName="Generalizes" isDerived="true">
					<targetConcept code="AN"/>
				</conceptRelationship>
				<conceptRelationship relationshipName="Generalizes" isDerived="true">
					<targetConcept code="ON"/>
				</conceptRelationship>
				<conceptProperty name="internalId" value="21054"/>
				<printName language="en" preferredForLanguage="true" text="ContextControlNonPropagating"/>
				<code code="_ContextControlNonPropagating" status="active"/>
			</concept>
			<concept isSelectable="false">
				<annotations>
					<documentation>
						<definition>
							<text>
