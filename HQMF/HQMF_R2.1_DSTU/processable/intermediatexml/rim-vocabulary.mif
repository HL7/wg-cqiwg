				<conceptProperty name="isDocumentCharacteristic" value="true"/>
				<conceptProperty name="Name:Act:outboundRelationship:ActRelationship" value="boundedSupport"/>
				<conceptProperty name="Sort:Act:outboundRelationship:ActRelationship" value="DAA______"/>
				<conceptProperty name="Name:Act:inboundRelationship:ActRelationship" value="boundedSupportOf"/>
				<conceptProperty name="Sort:Act:inboundRelationship:ActRelationship" value="DAA______"/>
				<printName language="en" preferredForLanguage="true" text="has bounded support"/>
				<code code="SPRTBND" status="active"/>
			</concept>
			<concept>
				<annotations>
					<documentation>
						<definition>
