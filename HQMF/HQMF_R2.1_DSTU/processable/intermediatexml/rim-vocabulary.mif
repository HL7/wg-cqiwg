				<conceptProperty name="isDocumentCharacteristic" value="true"/>
				<conceptProperty name="Name:Act:outboundRelationship:ActRelationship" value="verbatimExcerptFrom"/>
				<conceptProperty name="Sort:Act:outboundRelationship:ActRelationship" value="BMA______"/>
