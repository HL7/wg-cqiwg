				<conceptProperty name="Sort:Act:inboundRelationship:ActRelationship" value="BMA______"/>
				<printName language="en" preferredForLanguage="true" text="Excerpt verbatim"/>
				<code code="VRXCRPT" status="active"/>
			</concept>
		</releasedVersion>
	</codeSystem>
	<codeSystem name="ActStatus" title="ActStatus" codeSystemId="2.16.840.1.113883.5.14">
