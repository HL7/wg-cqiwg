				<conceptProperty name="isDocumentCharacteristic" value="true"/>
				<conceptProperty name="Name:Act:outboundRelationship:ActRelationship" value="excerptFrom"/>
				<conceptProperty name="Sort:Act:outboundRelationship:ActRelationship" value="BM_______"/>
				<conceptProperty name="Name:Act:inboundRelationship:ActRelationship" value="excerpt"/>
				<conceptProperty name="Sort:Act:inboundRelationship:ActRelationship" value="BM_______"/>
				<printName language="en" preferredForLanguage="true" text="Excerpts"/>
				<code code="XCRPT" status="active"/>
			</concept>
			<concept>
				<annotations>
					<documentation>
						<definition>
							<text>
								<p>Used when the target Act is a transformation of the source Act. (For instance, used to show that a CDA document is a transformation of a DICOM SR document.)</p>
							</text>
						</definition>
					</documentation>
				</annotations>
				<conceptRelationship relationshipName="Specializes">
					<targetConcept code="SEQL"/>
