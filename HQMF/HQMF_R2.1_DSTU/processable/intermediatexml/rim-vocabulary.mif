				<conceptProperty name="appliesTo" value="new head of thread"/>
				<conceptProperty name="howApplies" value="old head of thread"/>
				<conceptProperty name="isDocumentCharacteristic" value="true"/>
				<conceptProperty name="Name:Act:outboundRelationship:ActRelationship" value="updateOf"/>
				<conceptProperty name="Sort:Act:outboundRelationship:ActRelationship" value="BE_______"/>
				<conceptProperty name="Name:Act:inboundRelationship:ActRelationship" value="update"/>
				<conceptProperty name="Sort:Act:inboundRelationship:ActRelationship" value="BE_______"/>
				<printName language="en" preferredForLanguage="true" text="updates (condition)"/>
				<code code="UPDT" status="active"/>
			</concept>
			<concept>
				<annotations>
					<documentation>
						<definition>
							<text>
								<p>The source is an excerpt from the target.</p>
							</text>
						</definition>
					</documentation>
				</annotations>
				<conceptRelationship relationshipName="Specializes">
					<targetConcept code="SEQL"/>
